{
	"name": "vim",
	"description": "Vim emulation for VS Code",
	"version": "0.0.1",
	"publisher": "hjkl",
	"engines": {
		"vscode": "0.10.x"
	},
	"categories": [
		"Others"
	],
	"activationEvents": [
		"onCommand:extension.sayHello"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [{
			"command": "extension.sayHello",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "0.10.x"
	}
}